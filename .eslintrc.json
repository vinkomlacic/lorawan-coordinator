{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-invalid-this": "off",
        "max-len": "warn",
        "require-jsdoc": "warn",
        "linebreak-style": "off"
    }
}